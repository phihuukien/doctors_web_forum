@model Doctors_Web_Forum_FE.Models.Account@{    Layout = null;}<!DOCTYPE html><html lang="en" class="h-100"><head>    <meta charset="utf-8">    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">    <title> Sign In | Health Talk</title>    <link rel="shortcut icon" href="..\assets\svg\logos\logo.png">    <link href="~/assets/css/login.css" rel="stylesheet" />    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/all.min.css"></head><body style="background-image: linear-gradient(rgb(0 0 0 / 50%),rgb(0 0 0 / 50%)),url('assets/plus-symbol-background-4776812.jpg'); ">    <h2 style="color: #fbb7eb;">Health Talk: Sign in/up Form</h2>    <h3 style="color:green">@TempData["done"]</h3>    <div class="container @ViewBag.panelActive" id="container">        <div class="form-container sign-up-container">            <form asp-action="PostRegister">                <h1>Create Account</h1>                <hr>                <input type="text" asp-for="DisplayName" placeholder="Name" />                <span asp-validation-for="DisplayName" style="color:red"></span>                <input type="email" asp-for="Email" placeholder="Email" />                <span asp-validation-for="Email" style="color:red"></span>                <small style="color:red">@ViewBag.checkemail</small>                <input type="password" asp-for="Password" placeholder="Password(ex:12345Aa!)" id="password" />                <span class="fa fa-fw fa-eye field-icon toggle-password_1" style=" margin-top: -34px; position: relative; margin-left: 248px;"></span>                <span asp-validation-for="Password" style="color: red; margin-top: 24px; }"></span>                <input type="password" name="check_password" placeholder="Check Password" />                <small style="color:red">@ViewBag.checkpassword</small>                <lable style="width: 100%; text-align: start; font-size: 13px; margin-left: 18px;">Access Rights</lable>                <div style="width:100%">                    <div style="display: flex; align-items: center; width: 50%;">                        <input asp-for="Access_rights" type="radio" id="access_right_1" value="true" checked /><span>Private</span>                        <input asp-for="Access_rights" type="radio" id="access_right_2" value="false" /><span>Public</span>                    </div>                </div>                <hr />                <button type="submit">Sign Up</button>            </form>        </div>        <div class="form-container sign-in-container">            <form action="/login/authentication" method="post">                <h1>Sign in</h1>                <hr>                <small style="color:red;">@TempData["error"]</small>                <input type="hidden" value="@ViewBag.urlRedirect" name="urlRedirect" />                <input type="email" name="Email" placeholder="Email" value="@TempData["email"]" />                <input type="password" name="Password" id="passwordLogIn" placeholder="Password" value="@TempData["password"]" />                <span class="fa fa-fw fa-eye field-icon toggle-password"></span>                <hr />                <a href="#">Forgot your password?</a>                <button type="submit">Sign In</button>            </form>        </div>        <div class="overlay-container">            <div class="overlay">                <div class="overlay-panel overlay-left">                    <h1>Welcome Back!</h1>                    <p>To keep connected with us please login with your personal info</p>                    <button class="ghost" id="signIn">Sign In</button>                    <a href="/"><button class="ghost" style="margin-top:10px;" id="">Go Home</button></a>                </div>                <div class="overlay-panel overlay-right">                    <h1>Hello, Friend!</h1>                    <p>Enter your personal details and start journey with us</p>                    <button class="ghost" id="signUp">Sign Up</button>                    <a href="/"><button class="ghost" style="margin-top:10px;" id="">Go Home</button></a>                </div>            </div>        </div>    </div>    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>    <script src="~/assets/js/login.js"></script>    <script>        $(".toggle-password").click(function () {            $(this).toggleClass("fa-eye fa-eye-slash");            var x = document.getElementById("passwordLogIn");            if (x.type === "password") {                x.type = "text";            } else {                x.type = "password";            }        });        $(".toggle-password_1").click(function () {            $(this).toggleClass("fa-eye fa-eye-slash");            var x = document.getElementById("password");            if (x.type === "password") {                x.type = "text";            } else {                x.type = "password";            }        });    </script></body></html>