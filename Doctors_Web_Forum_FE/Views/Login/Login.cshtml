@model Doctors_Web_Forum_FE.Models.Account@{ Layout = null;}<!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <meta name="viewport" content="width=device-width, initial-scale=1.0">    <title>Register</title>    <link rel="shortcut icon" href="..\assets\svg\logos\logo.png">    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600&display=swap" rel="stylesheet">    <link href="~/assets/css/Register.css" rel="stylesheet" /></head><body>    <h2>Health Talk: Sign in/up Form</h2>    <h3 style="color:green">@TempData["done"]</h3>    <div class="container @ViewBag.panelActive" id="container">        <div class="form-container sign-up-container">            <form asp-action="PostRegister">                <h1>Create Account</h1>                <hr>                <input type="text" asp-for="DisplayName" placeholder="Name" />                <span asp-validation-for="DisplayName" style="color:red"></span>                <input type="email" asp-for="Email" placeholder="Email" />                <span asp-validation-for="Email" style="color:red"></span>                <small style="color:red">@ViewBag.checkemail</small>                <input type="password" asp-for="Password" placeholder="Password(ex:12345Aa!)" />                <span asp-validation-for="Password" style="color:red"></span>                <input type="password" name="check_password" placeholder="Check Password" />                <small style="color:red">@ViewBag.checkp</small>                <lable style="width: 100%; text-align: start; font-size: 13px; margin-left: 18px;">Access Rights</lable>                <div style="width:100%">                    <div style="display: flex; align-items: center; width: 50%;">                        <input asp-for="Access_rights" type="radio" id="access_right_1" value="true" checked /><span>Private</span>                        <input asp-for="Access_rights" type="radio" id="access_right_2" value="false" /><span>Public</span>                    </div>                </div>                <hr />                <button type="submit">Sign Up</button>            </form>        </div>        <div class="form-container sign-in-container">            <form action="#">                <h1>Sign in</h1>                <hr>                <input type="email" placeholder="Email" />                <input type="password" placeholder="Password" />                <a href="#">Forgot your password?</a>                <button>Sign In</button>            </form>        </div>        <div class="overlay-container">            <div class="overlay">                <div class="overlay-panel overlay-left">                    <h1>Welcome Back!</h1>                    <p>To keep connected with us please login with your personal info</p>                    <button class="ghost" id="signIn">Sign In</button>                </div>                <div class="overlay-panel overlay-right">                    <h1>Hello, Friend!</h1>                    <p>Enter your personal details and start journey with us</p>                    <button class="ghost" id="signUp">Sign Up</button>                </div>            </div>        </div>    </div>    <script>        const signUpButton = document.getElementById('signUp');        const signInButton = document.getElementById('signIn');        const container = document.getElementById('container');        signUpButton.addEventListener('click', () => {            container.classList.add("right-panel-active");        });        signInButton.addEventListener('click', () => {            container.classList.remove("right-panel-active");        });    </script></body></html>